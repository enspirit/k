assert(Data('{"name": "Alice"}') == {name: 'Alice'})
assert(Data('[1, 2, 3]') == [1, 2, 3])
assert(Data('{"user": {"name": "Bob"}}').user.name == 'Bob')
assert(Data('{"active": true}').active == true)
assert(Data('{"count": 42}').count == 42)
assert(Data([1, 2, 3]) == [1, 2, 3])
assert(Data({x: 10}).x == 10)
assert(isNull(Data('not valid json')))
assert(isNull(Data(null)))
assert(Data('invalid') | {default: true} == {default: true})
