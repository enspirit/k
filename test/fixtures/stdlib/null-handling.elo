assert(isNull(null) == true)
assert(isNull(42) == false)
assert(isNull('hello') == false)
assert(isNull(true) == false)
assert(typeOf(null) == 'Null')
assert((42 | 0) == 42)
assert(('hello' | 'default') == 'hello')
assert((1 + 1 | 0) == 2)
assert(indexOf('hello', 'l') == 2)
assert(isNull(indexOf('hello', 'l')) == false)
assert(isNull(indexOf('hello', 'x')) == true)
assert(typeOf(indexOf('hello', 'x')) == 'Null')
assert((indexOf('hello', 'x') | -1) == -1)
assert((indexOf('hello', 'l') | -1) == 2)
