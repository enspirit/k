assert(length('hello') == 5)
assert(length('') == 0)
assert(length('abc def') == 7)
assert(upper('hello') == 'HELLO')
assert(lower('WORLD') == 'world')
assert(upper('MiXeD') == 'MIXED')
assert(lower('MiXeD') == 'mixed')
assert(trim('  hello  ') == 'hello')
assert(trim('no spaces') == 'no spaces')
assert(trim('   ') == '')
assert(startsWith('hello world', 'hello'))
assert(!startsWith('hello world', 'world'))
assert(endsWith('hello world', 'world'))
assert(!endsWith('hello world', 'hello'))
assert(contains('hello world', 'lo wo'))
assert(contains('hello world', 'hello'))
assert(!contains('hello world', 'xyz'))
assert(substring('hello', 0, 2) == 'he')
assert(substring('hello', 1, 3) == 'ell')
assert(substring('hello world', 6, 5) == 'world')
assert(substring('hello', 2, 100) == 'llo')  # excessive length
assert(concat('hello', ' world') == 'hello world')
assert(concat('', 'test') == 'test')
assert(indexOf('hello world', 'world') == 6)
assert(indexOf('hello world', 'o') == 4)
assert(isNull(indexOf('hello world', 'xyz')) == true)
assert(replace('hello world', 'world', 'there') == 'hello there')
assert(replace('abab', 'ab', 'x') == 'xab')
assert(replaceAll('abab', 'ab', 'x') == 'xx')
assert(isEmpty(''))
assert(!isEmpty('hello'))
assert(padStart('42', 5, '0') == '00042')
assert(padEnd('hi', 5, '.') == 'hi...')
assert('hello' + ' ' + 'world' == 'hello world')
assert(upper('hello' + ' ' + 'world') == 'HELLO WORLD')
assert(lower('HELLO' + ' ' + 'WORLD') == 'hello world')
assert(trim(' hello ' + ' world ') == 'hello  world')
assert(length('ab' + 'cd') == 4)
assert('hi' * 3 == 'hihihi')
assert(3 * 'hi' == 'hihihi')
assert('ab' * 2 == 'abab')
assert('' * 5 == '')
assert('x' * 0 == '')
