assert(reverse([1, 2, 3]) == [3, 2, 1])
assert(reverse([]) == [])
assert(reverse(['a', 'b', 'c']) == ['c', 'b', 'a'])
assert(reverse([1]) == [1])
assert(join(['a', 'b', 'c'], ',') == 'a,b,c')
assert(join(['hello', 'world'], ' ') == 'hello world')
assert(join(['one'], '-') == 'one')
assert(join([], ',') == '')
assert(split('a,b,c', ',') == ['a', 'b', 'c'])
assert(split('hello world', ' ') == ['hello', 'world'])
assert(split('one', ',') == ['one'])
assert(split('', ',') == [])
