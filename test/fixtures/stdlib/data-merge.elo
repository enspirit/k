assert(merge({}, {}) == {})
assert(merge({a: 1}, {}) == {a: 1})
assert(merge({}, {b: 2}) == {b: 2})
assert(merge({a: 1}, {b: 2}) == {a: 1, b: 2})
assert(merge({a: 1}, {a: 2}) == {a: 2})
assert(merge({a: 1, b: 2}, {b: 3, c: 4}) == {a: 1, b: 3, c: 4})
assert(merge({x: {nested: 1}}, {y: 2}) == {x: {nested: 1}, y: 2})
assert(merge({x: {a: 1}}, {x: {b: 2}}) == {x: {b: 2}})
assert(deepMerge({}, {}) == {})
assert(deepMerge({a: 1}, {}) == {a: 1})
assert(deepMerge({}, {b: 2}) == {b: 2})
assert(deepMerge({a: 1}, {b: 2}) == {a: 1, b: 2})
assert(deepMerge({a: 1}, {a: 2}) == {a: 2})
assert(deepMerge({x: {a: 1}}, {x: {b: 2}}) == {x: {a: 1, b: 2}})
assert(deepMerge({x: {a: 1, b: 1}}, {x: {b: 2}}) == {x: {a: 1, b: 2}})
assert(deepMerge({a: {b: {c: 1}}}, {a: {b: {d: 2}}}) == {a: {b: {c: 1, d: 2}}})
assert(deepMerge({x: [1, 2]}, {x: [3, 4]}) == {x: [3, 4]})
assert(deepMerge({a: 1}, {a: {b: 2}}) == {a: {b: 2}})
assert(deepMerge({a: {b: 2}}, {a: 1}) == {a: 1})
