assert(length([1, 2, 3]) == 3)
assert(length([]) == 0)
assert(at([1, 2, 3], 0) == 1)
assert(at([1, 2, 3], 2) == 3)
assert(first([1, 2, 3]) == 1)
assert(isNull(first([])) == true)
assert(last([1, 2, 3]) == 3)
assert(isNull(last([])) == true)
assert(isEmpty([]) == true)
assert(isEmpty([1]) == false)
assert(isNull(at([1, 2, 3], 100)) == true)  # out of bounds
assert(isNull(at([1, 2, 3], -1)) == true)   # negative index
assert([1, 2] + [3, 4] == [1, 2, 3, 4])
assert([] + [1, 2] == [1, 2])
assert([1, 2] + [] == [1, 2])
assert([] + [] == [])
assert(['a'] + ['b', 'c'] == ['a', 'b', 'c'])
