assert(Int(123) == 123)
assert(Int('123') == 123)
assert(Int('-42') == -42)
assert(typeOf(Int('abc')) == 'NoVal')
assert(Float(3.14) == 3.14)
assert(Float('3.14') == 3.14)
assert(Float('-2.5') == -2.5)
assert(Float('123') == 123.0)
assert(typeOf(Float('abc')) == 'NoVal')
assert(Bool(true) == true)
assert(Bool(false) == false)
assert(Bool('true') == true)
assert(Bool('false') == false)
assert(typeOf(Bool('yes')) == 'NoVal')
assert(Date(D2025-01-15) == D2025-01-15)
assert(Date('2025-01-15') == D2025-01-15)
assert(typeOf(Date('not-a-date')) == 'NoVal')
assert(Datetime(D2025-01-15T10:30:00) == D2025-01-15T10:30:00)
assert(Datetime('2025-01-15T10:30:00') == D2025-01-15T10:30:00)
assert(typeOf(Datetime('not-a-datetime')) == 'NoVal')
assert(Duration(P1D) == P1D)
assert(Duration('P1D') == P1D)
assert(Duration('PT2H30M') == PT2H30M)
assert(typeOf(Duration('not-a-duration')) == 'NoVal')
assert((Int('bad') | 0) == 0)
assert((Int('42') | 0) == 42)
